<div id="form_comment">
    {{form_start(form)}}
    <div class="container">
        <div class="row">
            <div class="col-sm-12 col-lg-6">
                <div class="row">
                    <div class="col-sm-12">{{form_row(form.title)}}</div>
                </div>
                <div class="row">
                    <div class="col-sm-12">{{form_row(form.description)}}</div>
                </div>
            </div>
            <div class="col-sm-12 col-lg-6">
                {# <div class="row">
                    <div class="col-sm-12">{{form_row(form.like)}}</div>
                </div> #}
                <div class="row">
                    <div class="col-sm-12">{{form_row(form.rating)}}</div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12" >{{form_widget(form.save)}}</div>
        </div>

    </div>
    {{form_end(form)}}
</div>
